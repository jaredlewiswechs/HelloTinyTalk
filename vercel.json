{
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.py" },
    { "src": "/static/(.*)", "dest": "/api/index.py" },
    { "src": "/ide", "dest": "/api/index.py" },
    { "src": "/blueprint", "dest": "/api/index.py" },
    { "src": "/", "dest": "/api/index.py" }
  ]
}
